    <Page.Resources>
        <converters:Getcutimage x:Key="cutmode"/>
    </Page.Resources>
    <Grid>
        <Viewbox Stretch="Fill" StretchDirection="Both">
            <Grid Height="880" Width="1920">
                <Popup IsOpen="{Binding OK}" Placement="Center" x:Name="okpop" StaysOpen="False" VerticalOffset="-150">
                    <Grid Background="#1FD83F" Width="400" Height="300">
                        <Label Content="OK" FontSize="200" Foreground="White" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                    </Grid>
                </Popup>
                <Popup IsOpen="{Binding NG}" Placement="Center"  x:Name="ngpop" StaysOpen="False" VerticalOffset="-150">
                    <Grid Background="#FF0000" Width="400" Height="300">
                        <Label Content="NG" FontSize="200" Foreground="White" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                    </Grid>
                </Popup>
                <Image Width="57" Height="26" Margin="128,100,1735,753" Source="{Binding Cut1,Mode=TwoWay,Converter={StaticResource cutmode}}"/>
                <ProgressBar x:Name="leftpro" Style="{DynamicResource ProgressBarStyle1}" Margin="128,150,1734,455" Value="{Binding Data0.singleL}"/>
                <Image Width="57" Height="26" Margin="1735,100,128,753" Source="{Binding Cut2,Mode=TwoWay,Converter={StaticResource cutmode}}"/>
                <ProgressBar x:Name="rightpro" Style="{DynamicResource ProgressBarStyle1}" Margin="1735,150,127,455" Value="{Binding Data0.singleR}"/>
                <ProgressBar x:Name="pbpro" Style="{DynamicResource ProgressBarStyle2}" Margin="0,30,0,826" Value="{Binding Progress}"/>
