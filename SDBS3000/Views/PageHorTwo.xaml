                <!--左边表格-->
                <local:MeaTable x:Name="leftcircle" Width="807" Height="270" Margin="101,502,1012,108" Logotype="0" LabelContent="{Binding [Plane1], Source={x:Static loc:LanguageManager.Instance}}" CurrentValue="{Binding Data0.fl}" CurrentAngle="{Binding Data0.ql}" LastValue="{Binding Data1.fl}" LastAngle="{Binding Data1.ql}" ShowUnit="{Binding Rotor.Yxldw,Mode=TwoWay}" Yxl="{Binding Rotor.Pmyyxl}" SliderValue="{Binding Data0.singleL}"  Decinum="{Binding Decnum}"/>

                <!--右边表格-->
                <local:MeaTable x:Name="rightcircle" Width="807" Height="270" Margin="1012,502,101,108"  Logotype="1" LabelContent="{Binding [Plane2], Source={x:Static loc:LanguageManager.Instance}}" CurrentValue="{Binding Data0.fr}" CurrentAngle="{Binding Data0.qr}" LastValue="{Binding Data1.fr}" LastAngle="{Binding Data1.qr}" ShowUnit="{Binding Rotor.Yxldw}" Yxl="{Binding Rotor.Pmeyxl}" SliderValue="{Binding Data0.singleR}"  Decinum="{Binding Decnum}"/>
